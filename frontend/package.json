{
	"name": "cornelia-plugin-v2",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"lint": "eslint",
		"start": "next start -p 3002 --https --ssl-cert ~/.office-addin-dev-certs/localhost.crt --ssl-key ~/.office-addin-dev-certs/localhost.key",
